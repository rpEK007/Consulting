.module.tab-pane.section#ourContacts
  section#google-map.section.google-map-2
    .container
      iframe(width='100%', height='350', frameborder='0', scrolling='no', marginheight='0', marginwidth='0', src='https://maps.google.com/maps?f=q&source=s_q&hl=en&geocode=&q=dhaka&aq=&sll=37.0625,-95.677068&sspn=38.963048,86.572266&ie=UTF8&hq=&hnear=Dhaka,+Dhaka+Division,+Bangladesh&t=m&z=10&ll=23.709921,90.407143&output=embed')
  section#contact-us.section.contact-us
    .container
      .row
        .col-md-4.wrapper-2          
          include contactForm.jade
        .col-md-8.wrapper-1#contactForm
          form.contact-form
            h5.subpage-title= getInTouch
            .row
              .col-md-4
                input.input.form-control(placeholder=name, type='text')
              .col-md-4
                input.input.form-control(placeholder='Email', type='text')
              .col-md-4
                input.input.form-control(placeholder=subject, type='text')
            textarea.form-control(placeholder=yourMessage, rows='3')
            button.btn.btn-flat.flat-color= submit
